<% include header %>
	<table class="ui striped table">
	 	<thead>
	    	<tr>
		      	<th>ID</th>
		      	<th>用户名</th>
		      	<th>头像</th>
	    	</tr>
	  	</thead>
		<tbody>
		 <% users.forEach(function(user){ %>
			<tr>
	    		<td><%=  user.id %></td>
	    		<td><%=  user.Email %></td>
	    		<td>
	    			<% if(user.avatar!=''){ %>
	    				<img src="http://vue.wclimb.site/images/avator/<%=  user.avatar %>.png" alt="">
	    			<% }else{ %>
	    				还没有上传头像！
	    			<% } %>
	    		</td>
	    	</tr>
		 <% }) %>
		</tbody>
	</table>
	<%- include("pagination",{type:'mobileUser'}) %>
<% include footer %>
